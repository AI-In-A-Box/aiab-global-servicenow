        <script><![CDATA[(function() {
    // Create a new inference record with same settings
    var newInference = new GlideRecord('u_ai_inference');
    newInference.newRecord();
    
    // Copy key fields
    newInference.u_type = current.u_type;
    newInference.u_prompt = current.u_prompt;
    newInference.u_table = current.u_table;
    newInference.u_target = current.u_target;
    newInference.u_requested_by = current.u_requested_by;
    newInference.u_model = current.u_model;
    newInference.u_tools_included = current.u_tools_included;
    
    // Set state to new so it processes
    newInference.u_state = 'new';
    
    // Insert the record
    var newId = newInference.insert();
    
    if (newId) {
        // Redirect to the new record
        action.setRedirectURL(newInference);
        gs.addInfoMessage('Created new inference ' + newInference.u_number + '. Processing...');
    } else {
        gs.addErrorMessage('Failed to create retry inference');
    }
})();]]></script>
