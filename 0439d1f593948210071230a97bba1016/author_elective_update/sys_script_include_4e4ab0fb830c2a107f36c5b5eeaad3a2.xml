        <name>aiabChunkText</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var aiabChunkText = Class.create();
aiabChunkText.prototype = {
    initialize: function() {},
    toArray: function(text, chunkSize) {
		if(!text){
			throw "No text given";
		}
		if(!chunkSize){
			throw "No chunkSize given";
		}
        var chunkedText = [];
        var i = 0;
        var n = text.length;
        while (i < n) {
            chunkedText.push(text.slice(i, i += chunkSize));
        }
        return chunkedText;
    },
    type: 'aiabChunkText'
};]]></script>
